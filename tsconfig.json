{
  "compilerOptions": {
    "target": "es2016",                
    "module": "commonjs",                          
    "esModuleInterop": true,                            
    "forceConsistentCasingInFileNames": true,          
    "strict": true,                                 
    "skipLibCheck": true,
	"outDir": "./dist",
	"allowImportingTsExtensions":true,
	"emitDeclarationOnly": false,
	"noEmit": true
  },
  "include": ["src/**/*"]
}
